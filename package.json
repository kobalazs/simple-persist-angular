{
  "name": "simple-persist-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm i --path projects/simple-persist/angular/ && ng build @simple-persist/angular",
    "publish": "cd ./dist/simple-persist/angular/ && npm publish && cd ../../../",
    "watch": "ng build --watch --configuration development",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint \"projects/**/src/**/*.ts\" --cache --cache-location .eslint/.eslintcache",
    "lint:fix": "npm run lint -- --fix"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@simple-persist/angular": "file:projects/simple-persist/angular",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular/cli": "~14.2.6",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jest": "^29.5.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^13.1.2",
    "ng-packagr": "^14.2.0",
    "typescript": "~4.7.2"
  }
}
